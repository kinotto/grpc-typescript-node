{
  "name": "grpc-typescript-node",
  "version": "0.0.1",
  "description": "A simpl grpc typescript node based app",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --watch --content-base src/ --mode development --open",
    "start-server": "node ./server/server.js",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "build:proto": "node protogen.js",
    "start-envoy": "docker build -t helloworld/envoy -f ./server/envoy.Dockerfile . && docker run -d -p 8080:8080 helloworld/envoy",
    "test": "karma start karma.config.js"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "typescript"
  ],
  "author": "Karim",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.3",
    "@grpc/proto-loader": "^0.3.0",
    "@improbable-eng/grpc-web": "^0.11.0",
    "@types/google-protobuf": "^3.7.2",
    "@types/jasmine": "2.8.7",
    "@types/node": "7.0.0",
    "awesome-typescript-loader": "5.2.0",
    "google-protobuf": "^3.10.0",
    "grpc": "^1.24.2",
    "grpc-web": "^1.0.0",
    "jasmine-core": "3.1.0",
    "karma": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "3.0.0",
    "source-map-loader": "0.2.3",
    "ts-protoc-gen": "^0.12.0",
    "tslint": "5.17.0",
    "tslint-loader": "3.6.0",
    "typescript": "3.5.1",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.11"
  }
}
